<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <UsingTask AssemblyFile="..\tools\nrconfig.exe" TaskName="NewRelicCustomInstrumentationFileTask" />

  <Target Name="GenerateNewRelicCustomInstrumentationFile" AfterTargets="Build">
    <Message Text="Generating New Relic custom instrumentation files" />

    <NewRelicCustomInstrumentationFileTask InputFiles="$(TargetPath)" OutputFile="$(OutputPath)CustomInstrumentation.xml">
      
    </NewRelicCustomInstrumentationFileTask>
  </Target>
</Project>